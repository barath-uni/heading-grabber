{
  "manifest_version": 2,
  "name": "Heading Grabber",
  "version": "1.0",
  "description": "A Chrome extension to extract and format headings from web pages",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}